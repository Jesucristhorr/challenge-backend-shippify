{
  "name": "challenge-backend-shippify",
  "version": "1.0.0",
  "description": "My solution for Shippify's backend challenge.",
  "main": "dist/_app.js",
  "scripts": {
    "start": "node dist/_app.js",
    "build:1-prisma-generate": "prisma generate",
    "build:2-prisma-migrate": "prisma migrate deploy",
    "build:3-build-server": "rimraf dist && tsc",
    "build": "run-s build:*",
    "dev-db:1-prisma-generate": "prisma generate",
    "dev-db:2-prisma-migrate": "prisma migrate dev",
    "dev:1-prisma-studio": "prisma studio",
    "dev:2-serve": "nodemon",
    "dev": "run-s dev-db:* && run-p dev:*",
    "lint": "eslint ./src --ext .ts",
    "format": "prettier --config .prettierrc 'src/**/*.ts' --write",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "prisma": {
    "seed": "ts-node --project tsconfig.json prisma/seed.ts"
  },
  "nodemonConfig": {
    "watch": [
      "src"
    ],
    "ext": ".ts,.js",
    "ignore": [],
    "exec": "ts-node src/_app.ts"
  },
  "keywords": [
    "challenge",
    "backend",
    "nodejs",
    "typescript"
  ],
  "author": "Jes√∫s Moreira <jesus.moreira@codefilia.com>",
  "license": "MIT",
  "dependencies": {
    "@prisma/client": "^3.14.0",
    "cors": "^2.8.5",
    "express": "^4.18.1",
    "morgan": "^1.10.0",
    "zod": "^3.16.0",
    "zod-express-middleware": "^1.4.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/morgan": "^1.9.3",
    "@types/node": "^17.0.33",
    "@typescript-eslint/eslint-plugin": "^5.23.0",
    "@typescript-eslint/parser": "^5.23.0",
    "dotenv": "^16.0.1",
    "eslint": "^8.15.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-sonarjs": "^0.13.0",
    "nodemon": "^2.0.16",
    "npm-run-all": "^4.1.5",
    "prisma": "^3.14.0",
    "prisma-dbml-generator": "^0.9.0",
    "rimraf": "^3.0.2",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.4"
  }
}
